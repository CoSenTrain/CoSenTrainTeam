<sqlMap namespace="zipcode">
	<typeAlias alias="zip" type="bean.lim.ZipCode"/>
 <select id="getZipCode" resultClass="zip">
  select * from zipcode
 </select>

 <select id="selectZipCode" parameterClass="java.lang.String" resultClass="zip">
   select zipcode,sido ,gugun,dong, nvl(bunji,' ')bunji,zipcodeno from zipcode where dong like '%'||#dong#||'%'
 </select>

 

