<?xml version="1.0" encoding="EUC-KR"?>
<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
 <sqlMap namespace="you">
	<typeAlias alias="yusil" type="bean.cho.YusilBean"/> 
	
	<select id="selectYusil" resultClass="yusil">
		SELECT NO,TITLE,CATEGORY,STORE,REGDATE,CONTENTS 
		FROM BOARD
		WHERE TYPE='lost'
	</select>
 
 </sqlMap>